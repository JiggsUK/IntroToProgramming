import java.util.Arrays;

public class Week5 {
  public static void main(String[] args) {
        Week5 testRun = new Week5();
        testRun.task4();
    }
    public int[][] task4(){
        int[][] matrix = new int [2][3];
        int count = 1;
        while (count < 7) {
            for (int a = 0; a <= 1; a++) {
                for (int b = 0; b <= 2; b++) {
                    matrix[a][b] = count;
                    count++;
                }
            }
        }
        
        return matrix;
}
}